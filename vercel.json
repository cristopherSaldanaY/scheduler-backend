{
    "version": 2,
    "builds": [
      {
        "src": "./dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/organization",
        "dest": "/dist/index.js"
      },
      {
        "src": "/user",
        "dest": "/dist/index.js"
      },
      {
        "src": "/driver",
        "dest": "/dist/index.js"
      },
      {
        "src": "/vehicle",
        "dest": "/dist/index.js"
      },
      {
        "src": "/route",
        "dest": "/dist/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  